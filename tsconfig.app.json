{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/test.ts", "src/**/*.spec.ts"],
  "paths": {
    "@app/*": ["app/*"],
    "@assets/*": ["assets/*"],
    "@env/*": ["environments/*"],
    "@shared/*": ["app/shared/*"],
    "@core/*": ["app/core/*"],
    "@store/*": ["app/store/*"],
    "@model/*": ["app/model/*"]
  }
}
